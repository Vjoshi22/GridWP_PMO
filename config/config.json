{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "project-grid-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/projectGrid/ProjectGridWebPart.js",
          "manifest": "./src/webparts/projectGrid/ProjectGridWebPart.manifest.json"
        }
      ]
    },
    "enter-details-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/enterDetails/EnterDetailsWebPart.js",
          "manifest": "./src/webparts/enterDetails/EnterDetailsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js",
      "globalName": "jQuery"
    },
    "bootstrap": {
      "path": "node_modules/bootstrap/dist/js/bootstrap.min.js",
      "globalName": "bootstrap"
    }
  },
  "localizedResources": {
    "ProjectGridWebPartStrings": "lib/webparts/projectGrid/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "EnterDetailsWebPartStrings": "lib/webparts/enterDetails/loc/{locale}.js"
  }
}
